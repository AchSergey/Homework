package ru.inno.course.basics;

public class lesson1 {

    public static void main(String[] args) {

        String cardNumber = "1234 5678 8909 4321";
        String trimmedCardNumber = cardNumber.replaceAll(" ", "");
        String lastFourDigits = trimmedCardNumber.substring(trimmedCardNumber.length() - 4);
        String maskedDigits = "**** **** **** ";

        System.out.println(maskedDigits + lastFourDigits);
    }
}
